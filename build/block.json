{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "dss/jobbnorge",
  "version": "2.2.2",
  "title": "Jobbnorge",
  "category": "widgets",
  "icon": "people",
  "description": "Retrieve and display job listings from Jobbnorge.no",
  "keywords": [
    "jobbnorge",
    "jobbnorge.no"
  ],
  "supports": {
    "html": false
  },
  "attributes": {
    "columns": {
      "type": "number",
      "default": 3
    },
    "blockLayout": {
      "type": "string",
      "default": "list"
    },
    "employerID": {
      "type": "string",
      "default": "",
      "role": "content"
    },
    "noJobsMessage": {
      "type": "string",
      "default": ""
    },
    "orderBy": {
      "type": "string",
      "default": "Deadline"
    },
    "itemsToShow": {
      "type": "number",
      "default": 5
    },
    "displayEmployer": {
      "type": "boolean",
      "default": false
    },
    "displayExcerpt": {
      "type": "boolean",
      "default": true
    },
    "displayDeadline": {
      "type": "boolean",
      "default": false
    },
    "displayScope": {
      "type": "boolean",
      "default": false
    },
    "displayDate": {
      "type": "boolean",
      "default": true
    },
    "excerptLength": {
      "type": "number",
      "default": 55
    },
    "enablePagination": {
      "type": "boolean",
      "default": true
    },
    "jobsPerPage": {
      "type": "number",
      "default": 10
    }
  },
  "textdomain": "wp-jobbnorge-block",
  "editorScript": "file:init.js",
  "editorStyle": "file:editor.css",
  "style": "file:style-init.css"
}